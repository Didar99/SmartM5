        app:layout_constraintTop_toTopOf="@+id/t_LIntensive" />

    <TextView
        android:id="@+id/one_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="5dp"
        android:text="@string/_1"
        android:textColor="@color/bold_text"
        android:textSize="@dimen/_8font_ssp"
        android:textStyle="bold"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/air_seek"
        app:layout_constraintEnd_toEndOf="@+id/air_seek"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="@+id/air_seek"
        app:layout_constraintTop_toTopOf="@+id/air_seek" />

    <TextView
        android:id="@+id/two_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="5dp"
        android:text="@string/_2"
        android:textColor="@color/bold_text"
        android:textSize="@dimen/_8font_ssp"
        android:textStyle="bold"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/air_seek"
        app:layout_constraintEnd_toEndOf="@+id/air_seek"
        app:layout_constraintStart_toStartOf="@+id/air_seek"
        app:layout_constraintTop_toTopOf="@+id/air_seek"
        app:layout_constraintVertical_bias="0.508" />

    <SeekBar
        android:id="@+id/air_seek"
        android:layout_width="@dimen/_180hsdp"
        android:layout_height="@dimen/_57hsdp"
        android:layout_marginStart="@dimen/_43hsdp"
        android:layout_marginBottom="@dimen/_35hsdp"
        android:max="3"
        android:progress="0"
        android:progressDrawable="@drawable/seek_light"
        android:rotation="0"
        android:thumb="@drawable/thumb"
        android:thumbOffset="-5dp"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

