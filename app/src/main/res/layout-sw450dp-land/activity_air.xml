        android:textColor="@color/light_text"
        android:textSize="@dimen/_8font_ssp"
        app:backgroundTint="@color/transparent"
        app:cornerRadius="@dimen/_100hsdp"
        app:layout_constraintBottom_toBottomOf="@+id/b_speed2"
        app:layout_constraintEnd_toStartOf="@+id/b_speed2"
        app:layout_constraintStart_toStartOf="@+id/imageView82"
