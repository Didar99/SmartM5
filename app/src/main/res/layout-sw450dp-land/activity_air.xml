<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/app_background"
    tools:ignore="MissingDefaultResource">


    <com.unity.tabletgorjav.Croller
        android:id="@+id/recycle"
        android:layout_width="@dimen/_130hsdp"
        android:layout_height="@dimen/_130hsdp"
        android:layout_centerInParent="true"
        android:rotation="0"
        app:back_circle_color="#00B0FF"
        app:back_circle_radius="30"
        app:enabled="true"
        app:indicator_color="@color/bold_text"
        app:indicator_width="5"
        app:is_continuous="true"
        app:label=" "
        app:label_color="#000000"
        app:label_disabled_color="#8DDF2000"
        app:label_style="italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:main_circle_color="@color/app_background"
        app:max="20"
        app:progress_primary_color="#00B0FF"
        app:progress_primary_stroke_width="35"
        app:progress_secondary_color="@color/selected_text"
        app:progress_secondary_stroke_width="30"
        app:start_offset="45" />

    <TextView
        android:id="@+id/tmp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/recycle"
        android:layout_alignTop="@+id/recycle"
        android:fontFamily="@font/montserrat"
        android:textColor="@color/light_text"
        android:textSize="@dimen/_8font_ssp"
        app:layout_constraintBottom_toBottomOf="@+id/recycle"
        app:layout_constraintEnd_toEndOf="@+id/recycle"
        app:layout_constraintStart_toStartOf="@+id/recycle"
        app:layout_constraintTop_toTopOf="@+id/recycle" />


    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="@dimen/_140hsdp"
        android:layout_height="@dimen/_85hsdp"
        android:background="@drawable/round_panel_mini"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView14"
        app:layout_constraintHorizontal_bias="0.7"
        app:layout_constraintStart_toStartOf="@+id/imageView14"
        app:layout_constraintTop_toTopOf="@+id/recycle"
        app:layout_constraintVertical_bias="0.65" />

    <ImageView
        android:id="@+id/imageView82"
        android:layout_width="@dimen/_140hsdp"
        android:layout_height="@dimen/_85hsdp"
        android:background="@drawable/round_panel_mini"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView13"
        app:layout_constraintHorizontal_bias="0.3"
        app:layout_constraintStart_toStartOf="@+id/imageView13"
        app:layout_constraintTop_toTopOf="@+id/recycle"
        app:layout_constraintVertical_bias="0.65" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="@dimen/_140hsdp"
        android:layout_height="@dimen/_85hsdp"
        android:background="@drawable/round_panel_mini"
        app:layout_constraintBottom_toBottomOf="@+id/recycle"
        app:layout_constraintEnd_toStartOf="@+id/recycle"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="@dimen/_140hsdp"
        android:layout_height="@dimen/_85hsdp"
        android:background="@drawable/round_panel_mini"
        app:layout_constraintBottom_toBottomOf="@+id/recycle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.58"
        app:layout_constraintStart_toEndOf="@+id/recycle"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/imageViewd2"
        style="@style/Widget.MaterialComponents.Button"
        android:layout_width="@dimen/_30hsdp"
        android:layout_height="@dimen/_39hsdp"
        android:elevation="8dp"
        app:backgroundTint="@color/transparent"
        app:cornerRadius="@dimen/_100hsdp"
        app:icon="@drawable/ic_wind"
        app:iconSize="@dimen/_18hsdp"
        app:iconTint="@color/teal_200"
        app:layout_constraintBottom_toBottomOf="@+id/imageViewd"
        app:layout_constraintEnd_toEndOf="@+id/imageView13"
        app:layout_constraintStart_toEndOf="@+id/imageViewd"
        app:layout_constraintTop_toTopOf="@+id/imageViewd"
        app:rippleColor="@color/teal_200"
        app:strokeColor="#E1E1E1"
        app:strokeWidth="1dp" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/imageViewd"
        style="@style/Widget.MaterialComponents.Button"
        android:layout_width="@dimen/_30hsdp"
        android:layout_height="@dimen/_39hsdp"
        android:elevation="8dp"
        app:backgroundTint="@color/transparent"
        app:cornerRadius="@dimen/_100hsdp"
        app:icon="@drawable/ic_sunny"
        app:iconSize="@dimen/_18hsdp"
        app:iconTint="#FFC400"
        app:layout_constraintBottom_toBottomOf="@+id/imageView13"
        app:layout_constraintEnd_toEndOf="@+id/imageView13"
        app:layout_constraintStart_toStartOf="@+id/imageView13"
        app:layout_constraintTop_toTopOf="@+id/imageView13"
        app:rippleColor="#FFC400"
        app:strokeColor="#E1E1E1"
        app:strokeWidth="1dp" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/imageViewd12"
        style="@style/Widget.MaterialComponents.Button"
        android:layout_width="@dimen/_30hsdp"
        android:layout_height="@dimen/_39hsdp"
        android:elevation="8dp"
        app:backgroundTint="@color/transparent"
        app:cornerRadius="@dimen/_100hsdp"
        app:icon="@drawable/ic_snow"
        app:iconSize="@dimen/_15hsdp"
        app:iconGravity="end"
        app:iconTint="#2979FF"
        app:layout_constraintBottom_toBottomOf="@+id/imageViewd"
        app:layout_constraintEnd_toStartOf="@+id/imageViewd"
        app:layout_constraintStart_toStartOf="@+id/imageView13"
        app:layout_constraintTop_toTopOf="@+id/imageViewd"
        app:rippleColor="#00B0FF"
        app:strokeColor="#E1E1E1"
        app:strokeWidth="1dp" />


    <TextView
        android:id="@+id/t_LightData9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="8dp"
        android:fontFamily="@font/montserrat"
        android:text="Temp."
        android:textColor="@color/light_text"
        android:textSize="@dimen/_8font_ssp"
