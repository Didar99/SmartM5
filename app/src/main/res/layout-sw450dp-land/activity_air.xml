    <TextView
        android:id="@+id/t_LEnergy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:elevation="8dp"
        android:fontFamily="@font/montserrat"
        android:text="@string/wastedEnergy"
        android:textColor="@color/light_text"
        android:textSize="@dimen/_8font_ssp"
        app:layout_constraintStart_toStartOf="@+id/t_LIntensive2"
        app:layout_constraintTop_toBottomOf="@+id/t_LIntensive2" />

    <TextView
        android:id="@+id/t_LEnergyCost5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:elevation="8dp"
        android:fontFamily="@font/montserrat"
        android:text="@string/_21_40_pm"
        android:textColor="@color/light_text"
        android:textSize="@dimen/_8font_ssp"
        app:layout_constraintBottom_toBottomOf="@+id/t_LightData3"
        app:layout_constraintStart_toEndOf="@+id/t_LightData3"
        app:layout_constraintTop_toTopOf="@+id/t_LightData3" />

    <TextView
        android:id="@+id/t_LEnergyCost6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:elevation="8dp"
        android:fontFamily="@font/montserrat"
        android:text="@string/_18_00_pm"
        android:textColor="@color/light_text"
        android:textSize="@dimen/_8font_ssp"
        app:layout_constraintBottom_toBottomOf="@+id/t_LightData4"
        app:layout_constraintStart_toEndOf="@+id/t_LightData4"
        app:layout_constraintTop_toTopOf="@+id/t_LightData4" />

    <TextView
        android:id="@+id/t_LEnergyCost3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="8dp"
        android:fontFamily="@font/montserrat"
        android:text="@string/kwt21"
        android:textColor="@color/light_text"
        android:textSize="@dimen/_8font_ssp"
        app:layout_constraintBottom_toBottomOf="@+id/t_LEnergy"
        app:layout_constraintEnd_toEndOf="@+id/t_LEnergyCost4"
        app:layout_constraintTop_toTopOf="@+id/t_LEnergy" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/imageView9"
        style="@style/Widget.MaterialComponents.Button"
        android:layout_width="@dimen/_22hsdp"
        android:layout_height="@dimen/_32hsdp"
        android:layout_marginStart="16dp"
        android:elevation="18dp"
        android:paddingHorizontal="@dimen/_5hsdp"
        app:backgroundTint="@color/white"
        app:cornerRadius="12dp"
        app:icon="@drawable/ic_create"
        app:iconSize="@dimen/_12hsdp"
        app:iconTint="@color/gray_600"
        app:layout_constraintBottom_toBottomOf="@+id/imageView8"
        app:layout_constraintStart_toEndOf="@+id/t_LEnergyCost5"
        app:layout_constraintTop_toTopOf="@+id/imageView8"
        app:rippleColor="#E6E6E6"
        app:strokeColor="@color/selected_text"
        app:strokeWidth="2dp" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/imageView91"
        style="@style/Widget.MaterialComponents.Button"
        android:layout_width="@dimen/_22hsdp"
        android:layout_height="@dimen/_32hsdp"
        android:layout_marginStart="8dp"
        android:elevation="18dp"
        android:paddingHorizontal="@dimen/_5hsdp"
        app:backgroundTint="@color/selected_text"
        app:cornerRadius="12dp"
        app:icon="@drawable/ic_clear"
        app:iconSize="@dimen/_12hsdp"
        app:iconTint="@color/gray_600"
        app:layout_constraintBottom_toBottomOf="@+id/imageView9"
        app:layout_constraintStart_toEndOf="@+id/imageView9"
        app:layout_constraintTop_toTopOf="@+id/imageView9"
        app:rippleColor="#FFFFFF"
        app:strokeColor="@color/selected_text"
        app:strokeWidth="2dp" />

    <TextView
        android:id="@+id/t_LEnergyCost4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="8dp"
        android:fontFamily="@font/montserrat"
        android:text="@string/current_temp"
        android:textColor="@color/light_text"
        android:textSize="@dimen/_8font_ssp"
        app:layout_constraintBottom_toBottomOf="@+id/t_LIntensive2"
        app:layout_constraintEnd_toEndOf="@+id/t_LEnergyCost7"
        app:layout_constraintTop_toTopOf="@+id/t_LIntensive2" />


    <TextView
        android:id="@+id/t_LEnergyCost7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:elevation="8dp"
        android:fontFamily="@font/montserrat"
